{
  "name": "taller-react-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "^5.5.4",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "gh-pages": "^1.2.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.4"
  },
  "scripts": {
